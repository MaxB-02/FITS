# NextAuth Configuration
NEXTAUTH_SECRET=your-super-secret-key-here-change-this-in-production
NEXTAUTH_URL=http://localhost:3000

# Google OAuth (Required for NextAuth)
GOOGLE_CLIENT_ID=your_google_oauth_client_id_here
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret_here

# Admin Email Allowlist (Required for NextAuth)
ADMIN_EMAILS=admin@example.com,another-admin@example.com

# AWS S3 Configuration (Required for production)
S3_BUCKET=your-s3-bucket-name
AWS_REGION=us-east-2

# Resend API Key for sending email notifications (optional)
RESEND_API_KEY=your_resend_api_key_here

# Your business email where inquiries will be sent (optional)
NOTIFICATION_EMAIL=your-email@example.com 